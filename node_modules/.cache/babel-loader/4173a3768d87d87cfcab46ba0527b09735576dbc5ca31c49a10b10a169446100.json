{"ast":null,"code":"import { ref, onMounted, computed } from 'vue';\nimport cancelData from '../../assets/data/cancel.json'; // cancel.json 파일\n\nexport default {\n  __name: 'CancelTable',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const todayCancelData = ref([]);\n    const headers = ref(['No.', '이름', '연락처', '이벤트명', '취소 사유', '승인여부']);\n    const statusFilter = ref(''); // 승인여부 필터 상태\n\n    const today = new Date().toISOString().split('T')[0]; // YYYY-MM-DD 형태로 오늘 날짜 추출\n\n    // 오늘 날짜의 데이터만 필터링\n    onMounted(() => {\n      todayCancelData.value = cancelData.filter(item => item.cancelRequestDate === today);\n    });\n\n    // 필터링된 데이터를 computed로 처리\n    const filteredData = computed(() => {\n      if (statusFilter.value) {\n        return todayCancelData.value.filter(item => item.Status === statusFilter.value);\n      }\n      return todayCancelData.value; // 기본값 = 데이터 전체\n    });\n    const __returned__ = {\n      todayCancelData,\n      headers,\n      statusFilter,\n      today,\n      filteredData,\n      ref,\n      onMounted,\n      computed,\n      get cancelData() {\n        return cancelData;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","computed","cancelData","todayCancelData","headers","statusFilter","today","Date","toISOString","split","value","filter","item","cancelRequestDate","filteredData","Status"],"sources":["C:/Users/bs010/portfolioVan/vue-first/dashboard/src/components/compo/CancelTable.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- 승인여부 필터 -->\r\n    <div class=\"cancelled-filter\">\r\n      <label for=\"status-filter\">취소 승인 여부 : </label>\r\n      <select v-model=\"statusFilter\" id=\"status-filter\">\r\n        <option value=\"\">전체</option>\r\n        <option value=\"취소 완료\">취소 완료</option>\r\n        <option value=\"취소 대기\">취소 대기</option>\r\n      </select>\r\n    </div>\r\n\r\n    <!-- 테이블 -->\r\n    <table>\r\n      <thead>\r\n        <tr>\r\n          <th v-for=\"(header, index) in headers\" :key=\"index\">{{ header }}</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"(row, index) in filteredData\" :key=\"index\">\r\n          <td>{{ index + 1 }}</td>\r\n          <td>{{ row.name }}</td>\r\n          <td>{{ row.contact }}</td>\r\n          <td>{{ row.eventName }}</td>\r\n          <td>{{ row.cancelReason }}</td>\r\n          <td>{{ row.Status }}</td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, onMounted, computed } from 'vue';\r\nimport cancelData from '../../assets/data/cancel.json'; // cancel.json 파일\r\n\r\nconst todayCancelData = ref([]);\r\nconst headers = ref(['No.', '이름', '연락처', '이벤트명', '취소 사유', '승인여부']);\r\nconst statusFilter = ref(''); // 승인여부 필터 상태\r\n\r\nconst today = new Date().toISOString().split('T')[0]; // YYYY-MM-DD 형태로 오늘 날짜 추출\r\n\r\n// 오늘 날짜의 데이터만 필터링\r\nonMounted(() => {\r\n  todayCancelData.value = cancelData.filter(item => item.cancelRequestDate === today);\r\n});\r\n\r\n// 필터링된 데이터를 computed로 처리\r\nconst filteredData = computed(() => {\r\n  if (statusFilter.value) {\r\n    return todayCancelData.value.filter(item => item.Status === statusFilter.value);\r\n  }\r\n  return todayCancelData.value; // 기본값 = 데이터 전체\r\n});\r\n</script>\r\n\r\n<style scoped>\r\n  .cancelled-filter {\r\n    display: flex;\r\n    width: 100%;\r\n    justify-content: flex-end;\r\n    gap: 10px;\r\n  }\r\n</style>\r\n"],"mappings":"AAkCA,SAASA,GAAG,EAAEC,SAAS,EAAEC,QAAQ,QAAQ,KAAK;AAC9C,OAAOC,UAAU,MAAM,+BAA+B,CAAC,CAAC;;;;;;;;IAExD,MAAMC,eAAe,GAAGJ,GAAG,CAAC,EAAE,CAAC;IAC/B,MAAMK,OAAO,GAAGL,GAAG,CAAC,CAAC,KAAK,EAAE,IAAI,EAAE,KAAK,EAAE,MAAM,EAAE,OAAO,EAAE,MAAM,CAAC,CAAC;IAClE,MAAMM,YAAY,GAAGN,GAAG,CAAC,EAAE,CAAC,CAAC,CAAC;;IAE9B,MAAMO,KAAK,GAAG,IAAIC,IAAI,CAAC,CAAC,CAACC,WAAW,CAAC,CAAC,CAACC,KAAK,CAA